    "@types/commander": "^2.11.0",
    "@types/node": "^8.0.53",
    "@types/sax": "^1.0.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "mocha": "^4.0.1",
    "nyc": "^11.3.0",
    "prettier": "^1.8.2",
    "ts-node": "^3.3.0",
    "typescript": "^2.6.1"

  "description": "Vector Drawable & Animated Vector Drawable optimizer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "avdo": "./bin/avdo"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
